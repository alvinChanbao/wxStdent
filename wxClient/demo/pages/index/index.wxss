.page_ul {
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  align-items: center;
  justify-content: space-between;
  padding: 20rpx 30rpx;
}
.page_li {
  width: 300rpx;
  height: 400rpx;
  border: 2rpx solid #e5e5e5;
  margin-top: 40rpx;
  box-sizing: border-box;
  padding: 40rpx 30rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  border-radius: 10rpx;
}
.page_li_hover {
  background-color: rgba(1, 1, 1, 0.1);
}
.page_li_txt {
  color: #666;
  font-weight: bold;
  font-size: 30rpx;
}